  "scripts": {
    "build": "env NODE_ENV=production webpack --bail --progress",
    "build-dev": "webpack --bail --progress --devtool=source-map",
    "start": "webpack-dev-server --progress",
    "stats": "env NODE_ENV=production npm run stats-dev",
