{
  "name": "@gob/area-editor",
  "version": "3.0.0-rc.2",
  "description": "A course scheduling app for St. Olaf College",
  "homepage": "https://gobbldygook.site/",
  "repository": "hawkrives/gobbldygook",
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "build": "env NODE_ENV=production webpack --bail --progress",
    "build-dev": "webpack --bail --progress --devtool=source-map",
    "start": "webpack-dev-server --progress",
    "stats": "env NODE_ENV=production npm run stats-dev",
    "stats-dev": "webpack --bail --profile --json > stats.json && echo 'open http://webpack.github.io/analyse/'",
    "p": "cd .. && npm run p"
  },
  "peerDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "dependencies": {
    "@gob/hanson-format": "^3.0.0-rc.2",
    "@reach/component-component": "^0.1.1",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.11",
    "lz-string": "^1.4.4",
    "monaco-editor": "^0.14.3",
    "react-loadable": "^5.5.0",
    "stabilize": "^1.0.0",
    "styled-components": "^4.0.0-beta.8"
  },
  "devDependencies": {
    "@gob/webpack-plugin-html": "^1.0.0",
    "babel-loader": "^8.0.0",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "mini-css-extract-plugin": "^0.4.3",
    "monaco-editor-webpack-plugin": "^1.5.4",
    "style-loader": "^0.23.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8",
    "webpack-stylish": "^0.1.8"
  }
}
