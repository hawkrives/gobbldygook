    "babel-register": "6.24.1",
    "babel-stdin": "1.0.0",
    "case-sensitive-paths-webpack-plugin": "2.0.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cpy": "5.0.0",
    "cpy-cli": "1.0.1",
    "cross-env": "4.0.0",
