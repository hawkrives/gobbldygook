{
  "name": "gobbldygook",
  "version": "3.0.0-beta10.18",
  "description": "A course scheduling app for St. Olaf College",
  "homepage": "https://hawkrives.github.io/gobbldygook/",
  "main": "src/index.js",
  "private": true,
  "repository": "hawkrives/gobbldygook",
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "nyc": {
    "include": [
      "modules/**"
    ],
    "exclude": [
      "test",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**",
      "modules/hanson-format/parse-hanson-string.js"
    ],
    "cache": true,
    "sourceMap": false,
    "instrument": false
  },
  "jest": {
    "setupFiles": [
      "config/mocha-harness.js"
    ],
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text"
    ],
    "resetModules": true,
    "moduleNameMapper": {
      "^.+\\.(scss|css|less)$": "<rootDir>/config/flow/css.js.flow"
    },
    "modulePathIgnorePatterns": [
      "\\.scss$",
      "\\.support.js$",
      "__support__"
    ]
  },
  "scripts": {
    "build": "npm run copy && cross-env NODE_ENV=production webpack --config .webpackrc.js --bail --production --progress",
    "build-ci": "npm run build -- --no-progress",
    "build-dev": "cross-env NODE_ENV=development webpack --config .webpackrc.js --bail --progress --devtool=source-map",
    "build-peg": "pegjs --allowed-start-rules Result,Filter < modules/hanson-format/parse-hanson-string.pegjs | babel-stdin | js-beautify -f - --end-with-newline --indent-with-tabs > modules/hanson-format/parse-hanson-string.js",
    "copy": "rimraf build && mkdirp build/ && cpy \"modules/web/static/*\" build/",
    "cover": "npm run run-coverage && npm run view-coverage",
    "deploy": "bash scripts/deploy.sh",
    "lint": "eslint *.js modules/",
    "profile": "cat profile/README.md",
    "run-coverage": "nyc npm test",
    "serve-production": "cross-env NODE_ENV=production webpack-dev-server --config .webpackrc.js --progress",
    "start": "npm run copy && cross-env NODE_ENV=development webpack-dev-server --config .webpackrc.js --progress",
    "stats": "cross-env NODE_ENV=production npm run stats-base",
    "stats-base": "webpack --config .webpackrc.js --bail --profile --json > stats.json && echo 'open http://webpack.github.io/analyse/'",
    "stats-dev": "cross-env NODE_ENV=development npm run stats-base",
    "test": "cross-env NODE_ENV=test mocha --reporter dot --ui bdd --require babel-register --require config/mocha-harness modules/**/*.test.js",
    "jest": "cross-env NODE_ENV=test jest",
    "test-students": "./modules/cli/test-student test/example-students",
    "test-watch": "npm run test -- --watch",
    "travis": "npm run lint && npm run cover && npm run build-ci",
    "view-coverage": "nyc report --reporter=html && open coverage/index.html",
    "watch": "cross-env NODE_ENV=development webpack --watch --config .webpackrc.js --bail --progress --devtool=source-map"
  },
  "dependencies": {
    "appdirs": "1.0.0",
    "babel-runtime": "6.18.0",
    "bluebird": "3.4.6",
    "chalk": "1.1.3",
    "classnames": "2.2.5",
    "codecov": "^1.0.1",
    "codemirror": "5.20.0",
    "coveralls": "^2.11.14",
    "css-select": "1.2.0",
    "debug": "2.2.0",
    "delay": "1.3.1",
    "fuzzysearch": "1.0.3",
    "get-stdin": "5.0.1",
    "glob": "7.1.1",
    "graceful-fs": "4.1.9",
    "history": "2.1.2",
    "htmlparser2": "3.9.2",
    "humanize-plus": "1.8.2",
    "idb-range": "3.1.3",
    "idb-request": "3.2.0",
    "is-safari": "1.0.0",
    "js-yaml": "3.6.1",
    "junk": "2.0.0",
    "keymage": "1.1.3",
    "lodash": "4.16.4",
    "mkdirp": "0.5.1",
    "mocha-junit-reporter": "^1.12.1",
    "node-fetch": "1.6.3",
    "nomnom": "1.8.1",
    "ord": "0.1.1",
    "plur": "2.1.2",
    "present": "1.0.0",
    "pretty-ms": "2.1.0",
    "react": "15.3.2",
    "react-codemirror": "0.2.6",
    "react-dnd": "2.1.4",
    "react-dnd-html5-backend": "2.1.2",
    "react-document-title": "2.0.2",
    "react-dom": "15.3.2",
    "react-dropzone": "3.7.0",
    "react-input-autosize": "1.1.0",
    "react-modal": "1.5.2",
    "react-radio-group": "3.0.1",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-select": "1.0.0-beta13",
    "redux": "3.6.0",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.1.0",
    "redux-undo": "0.6.1",
    "serialize-error": "2.0.0",
    "sparkly": "3.1.2",
    "stabilize": "1.0.0",
    "sto-course-related-data": "4.0.2",
    "sto-sis-time-parser": "2.2.11",
    "text-table": "0.2.0",
    "treo": "0.6.0-rc2",
    "uuid": "2.0.3",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-jest": "16.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-external-helpers": "6.8.0",
    "babel-plugin-istanbul": "2.0.3",
    "babel-plugin-lodash": "3.2.9",
    "babel-plugin-transform-class-properties": "6.16.0",
    "babel-plugin-transform-inline-environment-variables": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-display-name": "6.8.0",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-plugin-webpack-alias": "1.9.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.16.3",
    "babel-stdin": "1.0.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chai": "3.5.0",
    "cpy-cli": "1.0.1",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "enzyme": "2.5.1",
    "eslint": "3.7.1",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-flowtype": "2.23.0",
    "eslint-plugin-flowtype-errors": "1.4.1",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "flow-bin": "0.33.0",
    "iflow-bluebird": "1.1.26",
    "iflow-lodash": "1.1.27",
    "jest": "16.0.2",
    "js-beautify": "1.6.4",
    "json-loader": "0.5.4",
    "mocha": "3.1.2",
    "mock-require": "1.3.0",
    "node-sass": "3.10.1",
    "nyc": "8.3.2",
    "offline-plugin": "3.4.2",
    "pegjs": "0.10.0",
    "react-addons-perf": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "redux-devtools": "3.3.1",
    "redux-freeze": "0.1.5",
    "redux-logger": "2.7.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2",
    "worker-loader": "0.7.1"
  }
}
