    "sparkly": "3.1.2",
    "style-loader": "0.17.0",
    "text-table": "0.2.0",
    "url-loader": "0.5.8",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5",
    "worker-loader": "0.8.0"
  }
}
