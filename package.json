{
  "name": "gobbldygook",
  "version": "3.0.0-beta.0",
  "description": "A course scheduling app for St. Olaf College",
  "main": "src/index.js",
  "private": true,
  "repository": "hawkrives/gobbldygook",
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "config": {
    "area_output": "build/areas/",
    "beautify": "-f - --end-with-newline --indent-with-tabs",
    "mocha": "test/areas test/models",
    "webpack": "--progress --config=./.webpack.config.js"
  },
  "scripts": {
    "benchmark": "babel-node ./benchmarks/students-benchmark.js --runs 50 --graph",
    "build": "NODE_ENV=production webpack -p webpack $npm_package_config_webpack",
    "build:areas": "./bin/compile-areas area-data/ --out-dir $npm_package_config_area_output",
    "build:peg": "pegjs < src/lib/parse-hanson-string.pegjs | babel --compact 'true' | js-beautify $npm_package_config_beautify > src/lib/parse-hanson-string.js",
    "clean": "rm -rf build/",
    "copy": "mkdirp build/ && cp -fR src/static/* build/ && npm run copy:areas",
    "copy:areas": "cp -rf ./area-data/ $npm_package_config_area_output && ./bin/area-package-maker $npm_package_config_area_output --save",
    "cover": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- $npm_package_config_mocha",
    "cover:view": "npm run cover && opn coverage/lcov-report/index.html",
    "coveralls": "npm run cover && coveralls < ./coverage/lcov.info",
    "lint": "eslint bin/ src/",
    "prebuild": "npm run clean && npm run copy",
    "prestart": "npm run copy",
    "prewatch": "npm run copy && npm run lint",
    "profile": "cat profile/README.md",
    "start": "webpack-dev-server $npm_package_config_webpack",
    "test": "mocha $npm_package_config_mocha",
    "test:students": "mocha ./bin/test-students",
    "travis": "npm run build:areas && npm run test:students && npm run build && npm test && npm run coveralls",
    "watch": "npm run watch:lint & npm run watch:test & wait"
  },
  "dependencies": {
    "babel-core": "5.8.22",
    "classnames": "2.1.3",
    "combinations-generator": "1.0.1",
    "debug": "2.2.0",
    "fuzzysearch": "1.0.3",
    "humanize-list": "1.0.0",
    "humanize-plus": "1.5.0",
    "idb-range": "2.4.0",
    "immutable": "3.7.4",
    "js-yaml": "3.3.1",
    "json-stable-stringify": "1.0.0",
    "keymage": "1.1.0",
    "lie": "3.0.1",
    "lodash": "3.10.1",
    "marked": "0.3.5",
    "node-uuid": "1.4.3",
    "ord": "0.1.1",
    "parse": "1.5.0",
    "plur": "2.0.0",
    "pouchdb": "4.0.0",
    "pouchdb-find": "0.3.5",
    "present": "0.0.6",
    "react": "0.13.3",
    "react-dnd": "1.1.4",
    "react-document-title": "1.0.2",
    "react-input-autosize": "0.5.0",
    "react-list-select": "0.3.0",
    "react-router": "0.13.3",
    "reflux": "0.2.12",
    "sha1": "1.1.1",
    "stickyfill": "1.1.1",
    "sto-helpers": "4.2.11",
    "sto-sis-time-parser": "2.1.0",
    "treo": "0.5.0",
    "whatwg-fetch": "0.9.0"
  },
  "devDependencies": {
    "autoprefixer-core": "5.2.1",
    "babel": "5.8.21",
    "babel-eslint": "4.0.5",
    "babel-loader": "5.3.2",
    "bn.js": "0.15.0",
    "chai": "3.2.0",
    "coveralls": "2.11.3",
    "css-loader": "0.15.6",
    "dom-storage": "2.0.1",
    "eslint": "1.1.0",
    "eslint-plugin-react": "3.2.1",
    "file-loader": "0.8.4",
    "glob": "5.0.14",
    "graceful-fs": "4.1.2",
    "hjs-webpack": "2.12.2",
    "isparta": "3.0.3",
    "js-beautify": "1.5.10",
    "json-loader": "0.5.2",
    "junk": "1.0.2",
    "matcha": "0.6.0",
    "meow": "3.3.0",
    "mkdirp": "0.5.1",
    "mocha": "2.2.5",
    "node-libs-browser": "0.5.2",
    "node-sass": "3.2.0",
    "nomnom": "1.8.1",
    "pegjs": "0.8.0",
    "postcss": "4.1.16",
    "postcss-calc": "4.1.0",
    "postcss-colormin": "1.2.4",
    "postcss-loader": "0.5.1",
    "pretty-hrtime": "1.0.0",
    "pretty-ms": "2.0.0",
    "react-hot-loader": "1.2.8",
    "sass-loader": "2.0.0",
    "sparkly": "2.0.1",
    "style-loader": "0.12.3",
    "stylus-loader": "1.2.1",
    "url-loader": "0.5.6",
    "webpack": "1.11.0",
    "webpack-dev-server": "1.10.1",
    "yeticss": "7.0.3"
  }
}
