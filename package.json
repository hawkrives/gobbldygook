    "react-dnd": "2.5.4",
    "react-dnd-html5-backend": "2.5.4",
    "react-document-title": "2.0.3",
    "react-dom": "16.2.0",
    "react-dropzone": "4.2.7",
    "react-input-autosize": "2.2.1",
    "react-modal": "3.1.11",
    "react-radio-group": "3.0.2",
    "react-redux": "5.0.6",
    "react-router": "3.2.0",
    "react-select": "1.2.1",
    "redux": "3.7.2",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.2.0",
    "redux-undo": "0.6.1",
