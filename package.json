{
  "name": "gobbldygook",
  "version": "3.0.0-beta10.18",
  "description": "A course scheduling app for St. Olaf College",
  "homepage": "https://hawkrives.github.io/gobbldygook/",
  "private": true,
  "repository": "hawkrives/gobbldygook",
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "./config/mocha-harness.js"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "collectCoverageFrom": [
      "modules/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/modules/hanson-format/parse-hanson-string.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "globals": {
      "DEVELOPMENT": false,
      "PRODUCTION": false,
      "TESTING": true,
      "VERSION": "3.0.0-test"
    },
    "resetModules": true,
    "moduleNameMapper": {
      "^.+\\.scss$": "<rootDir>/config/flow/css.js.flow"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "\\.support.js$",
      "__support__"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "history"
    ]
  },
  "config": {
    "prettier": "--trailing-comma es5 --tab-width 4 --single-quote --no-semi"
  },
  "scripts": {
    "build": "npm run copy && cross-env NODE_ENV=production webpack --config .webpackrc.js --bail --progress",
    "build-dev": "webpack --config .webpackrc.js --bail --progress --devtool=source-map",
    "build-peg": "pegjs --allowed-start-rules Result,Filter < modules/hanson-format/parse-hanson-string.pegjs | babel-stdin | js-beautify -f - --end-with-newline --indent-with-tabs > modules/hanson-format/parse-hanson-string.js",
    "copy": "rimraf build && cpy 'modules/web/static/*' build/",
    "cover": "jest --coverage && open coverage/index.html",
    "flow": "flow check",
    "lint": "eslint *.js modules/",
    "prettier": "prettier --write $npm_package_config_prettier 'modules/**/*.js' '*.js' '.*.js'",
    "prettier:changed": "FILES=$(git diff --name-only | grep '\\.js$'); prettier --write $npm_package_config_prettier $FILES",
    "prettier:since-master": "FILES=$(git diff --name-only master | grep '\\.js$'); prettier --write $npm_package_config_prettier $FILES",
    "serve-production": "cross-env NODE_ENV=production webpack-dev-server --config .webpackrc.js --progress",
    "start": "npm run copy && webpack-dev-server --config .webpackrc.js --progress",
    "stats": "cross-env NODE_ENV=production npm run stats-dev",
    "stats-dev": "webpack --config .webpackrc.js --bail --profile --json > stats.json && echo 'open http://webpack.github.io/analyse/'",
    "test": "jest",
    "test-students": "./modules/cli/test-student test/example-students",
    "watch": "webpack --watch --config .webpackrc.js --bail --progress --devtool=source-map"
  },
  "dependencies": {
    "babel-runtime": "6.23.0",
    "brwsr": "1.0.1",
    "classnames": "2.2.5",
    "codemirror": "5.25.2",
    "css-select": "1.2.0",
    "debug": "2.6.6",
    "dedent": "0.7.0",
    "delay": "1.3.1",
    "es6-promise": "4.1.0",
    "es6-set": "0.1.5",
    "fuzzysearch": "1.0.3",
    "history": "3.2.1",
    "htmlparser2": "3.9.2",
    "idb-range": "3.1.3",
    "idb-request": "3.2.0",
    "invariant": "2.2.2",
    "is-safari": "1.0.0",
    "js-yaml": "3.8.4",
    "keymage": "1.1.3",
    "listify": "1.0.0",
    "lodash": "4.17.4",
    "ord": "0.1.1",
    "p-props": "1.0.0",
    "p-series": "1.0.0",
    "p-settle": "2.0.0",
    "plur": "2.1.2",
    "present": "1.0.0",
    "pretty-ms": "2.1.0",
    "react": "15.5.4",
    "react-codemirror": "0.3.0",
    "react-dnd": "2.4.0",
    "react-dnd-html5-backend": "2.4.1",
    "react-document-title": "2.0.3",
    "react-dom": "15.5.4",
    "react-dropzone": "3.13.1",
    "react-input-autosize": "1.1.3",
    "react-modal": "1.7.7",
    "react-radio-group": "3.0.1",
    "react-redux": "5.0.4",
    "react-router": "3.0.2",
    "react-select": "1.0.0-rc.3",
    "redux": "3.6.0",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.2.0",
    "redux-undo": "0.6.1",
