{
  "name": "gobbldygook",
  "version": "3.0.0-rc.2",
  "description": "A course scheduling app for St. Olaf College",
  "homepage": "https://gobbldygook.site/",
  "private": true,
  "repository": "hawkrives/gobbldygook",
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "singleQuote": true,
    "semi": false,
    "bracketSpacing": false,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "useTabs": true
        }
      },
      {
        "files": "modules/web/styles/normalize.scss",
        "options": {
          "requirePragma": true
        }
      },
      {
        "files": "*.md",
        "options": {
          "requirePragma": true
        }
      },
      {
        "files": "flow-typed/**/*.js",
        "options": {
          "requirePragma": true
        }
      },
      {
        "files": "package.json",
        "options": {
          "requirePragma": true
        }
      }
    ]
  },
  "jest": {
    "setupFiles": [
      "./config/mocha-harness.js"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "collectCoverageFrom": [
      "modules/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/modules/hanson-format/parse-hanson-string.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "globals": {
      "DEVELOPMENT": false,
      "PRODUCTION": false,
      "TESTING": true,
      "VERSION": "3.0.0-test"
    },
    "resetModules": true,
    "moduleNameMapper": {
      "^.+\\.scss$": "<rootDir>/config/flow/css.js.flow"
    },
    "testPathIgnorePatterns": [
      "\\.support.js$",
      "__support__"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "history"
    ]
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config .webpackrc.js --bail --progress",
    "build-dev": "webpack --config .webpackrc.js --bail --progress --devtool=source-map",
    "build-peg": "pegjs --allowed-start-rules Result,Filter < modules/hanson-format/parse-hanson-string.pegjs | babel-stdin | prettier $npm_package_config_prettier --stdin > modules/hanson-format/parse-hanson-string.js",
    "bugsnag": "curl -d 'apiKey=7e393deddaeb885f5b140b4320ecef6b' -d 'repository=https://github.com/hawkrives/gobbldygook' -d 'revision=$(git rev-parse --verify HEAD)' 'https://notify.bugsnag.com/deploy'",
    "count": "echo $(fd -e scss | wc -l) files && echo $(fd -e scss | xargs wc -l | gsort | tail -n1) lines",
    "cover": "jest --coverage && open coverage/index.html",
    "flow": "flow",
    "lint": "eslint --cache *.js .*.js modules/",
    "netlify": "npm run build -- --no-progress && npm run bugsnag",
    "prettier": "prettier --write 'config/**/*.js' 'modules/**/*.{js,scss}' '{.,}*.js'",
    "prettier:changed": "FILES=$(git diff --name-only | grep '\\.js$'); prettier --write $FILES",
    "prettier:since-master": "FILES=$(git diff --name-only master | grep '\\.js$'); prettier --write $FILES",
    "serve-production": "cross-env NODE_ENV=production webpack-dev-server --config .webpackrc.js --progress",
    "start": "webpack-dev-server --config .webpackrc.js --progress",
    "stats": "cross-env NODE_ENV=production npm run stats-dev",
    "stats-dev": "webpack --config .webpackrc.js --bail --profile --json > stats.json && echo 'open http://webpack.github.io/analyse/'",
    "test": "jest",
    "test-students": "./modules/cli/test-student test/example-students",
    "watch": "webpack --watch --config .webpackrc.js --bail --progress --devtool=source-map"
  },
  "dependencies": {
    "babel-runtime": "6.26.0",
    "brwsr": "1.0.1",
    "classnames": "2.2.5",
    "codemirror": "5.34.0",
    "css-select": "1.2.0",
    "debug": "3.1.0",
    "dedent": "0.7.0",
    "delay": "2.0.0",
    "es6-promise": "4.2.4",
    "fuzzysearch": "1.0.3",
    "history": "3.3.0",
    "htmlparser2": "3.9.2",
    "idb-range": "3.1.3",
    "idb-request": "3.2.0",
    "invariant": "2.2.2",
    "is-safari": "1.0.0",
    "js-yaml": "3.10.0",
