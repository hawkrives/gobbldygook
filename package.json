    "ord": "0.1.1",
    "p-props": "1.1.0",
    "p-series": "1.0.0",
    "p-settle": "2.0.0",
    "plur": "2.1.2",
    "polished": "1.9.0",
    "present": "1.0.0",
    "pretty-ms": "3.1.0",
    "react": "16.2.0",
