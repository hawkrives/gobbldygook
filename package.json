{
  "name": "gobbldygook",
  "version": "3.0.0-beta5",
  "description": "A course scheduling app for St. Olaf College",
  "main": "src/index.js",
  "private": true,
  "repository": "hawkrives/gobbldygook",
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "scripts": {
    "build": "env NODE_ENV=production webpack --config ./.webpackrc.js --bail --production --progress",
    "build:areas": "./bin/compile-areas area-data/ --out-dir build/areas/",
    "build:ci": "npm run build -- --no-progress",
    "build:dev": "env NODE_ENV=development webpack --config ./.webpackrc.js --bail --progress --devtool=source-map",
    "build:peg": "pegjs < src/area-tools/parse-hanson-string.pegjs | ./scripts/babel-stdin.js | js-beautify -f - --end-with-newline --indent-with-tabs > src/area-tools/parse-hanson-string.js",
    "clean": "rm -rf build/* && rm -rf build/",
    "copy": "mkdir -p build/ && cp -rf src/static/* build/ && npm run copy:areas && npm run copy:icons",
    "copy:areas": "cp -rf ./area-data/ build/areas/ && ./bin/area-package-maker build/areas/ --save",
    "copy:icons": "svg-sprite-generate -d src/icons/ionicons/ -o build/ionicons.svg",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "codecov": "nyc report --reporter=text-lcov | codecov",
    "lint": "eslint *.js bin/ scripts/ src/ test/",
    "nyc": "nyc npm test",
    "prebuild": "npm run copy",
    "precopy": "npm run clean",
    "prestart": "npm run copy",
    "profile": "cat profile/README.md",
    "serve-production": "env NODE_ENV=production webpack-dev-server --config ./.webpackrc.js --progress",
    "start": "env NODE_ENV=development webpack-dev-server --config ./.webpackrc.js --progress",
    "stats": "env NODE_ENV=production npm run stats:base",
    "stats:base": "webpack --config ./.webpackrc.js --bail --profile --json > stats.json && echo 'open http://webpack.github.io/analyse/'",
    "stats:dev": "env NODE_ENV=development npm run stats:base",
    "test": "env NODE_ENV=test mocha test/**/*.js",
    "test-watch": "npm run test -- --watch",
    "test:students": "./bin/test-students",
    "view-coverage": "nyc report --reporter=html && open coverage/index.html"
  },
  "dependencies": {
