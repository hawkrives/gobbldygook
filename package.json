{
  "name": "gobbldygook",
  "version": "3.0.0-beta2",
  "description": "A course scheduling app for St. Olaf College",
  "main": "src/index.js",
  "private": true,
  "repository": "hawkrives/gobbldygook",
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "COURSE_INFO": "../sto-course-data/build/info.json",
  "AREA_INFO": "./build/areas/info.json",
  "config": {
    "area_output": "build/areas/",
    "beautify": "-f - --end-with-newline --indent-with-tabs",
    "mocha": "test/areas test/models test/helpers test/ducks",
    "webpack": "--progress --config .webpackrc"
  },
  "scripts": {
    "benchmark": "babel-node -- ./benchmarks/students-benchmark.js",
    "build": "env NODE_ENV=production webpack --production --bail $npm_package_config_webpack",
    "build:areas": "./bin/compile-areas area-data/ --out-dir $npm_package_config_area_output",
    "build:ci": "npm run build -- --no-progress",
    "build:peg": "pegjs < src/lib/parse-hanson-string.pegjs | babel --compact 'true' | js-beautify $npm_package_config_beautify > src/lib/parse-hanson-string.js",
    "clean": "rimraf build/",
    "copy": "mkdirp build/ && cp -rf src/static/* build/ && npm run copy:areas",
    "copy:areas": "cp -rf ./area-data/ $npm_package_config_area_output && ./bin/area-package-maker $npm_package_config_area_output --save",
    "cover": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- $npm_package_config_mocha",
    "cover:view": "npm run cover && open coverage/lcov-report/index.html",
    "coveralls": "npm run cover && coveralls < ./coverage/lcov.info",
    "eslint": "npm run lint",
    "karma": "env NODE_ENV=test karma start .karmarc --browsers Chrome,Firefox --single-run",
    "karma:ci": "env NODE_ENV=test karma start .karmarc --browsers Firefox --single-run",
    "lint": "eslint bin/ src/ test/",
    "mocha": "env NODE_ENV=test mocha $npm_package_config_mocha",
    "prebuild": "npm run copy",
    "precopy": "npm run clean",
    "prestart": "npm run copy",
    "prewatch": "npm run copy && npm run lint",
    "profile": "cat profile/README.md",
    "start": "webpack-dev-server $npm_package_config_webpack",
    "stats": "webpack --bail --config .webpackrc --profile --json > stats.json && open http://webpack.github.io/analyse/",
    "test": "npm run mocha",
    "test:browser": "npm run karma",
    "test:students": "mocha ./bin/test-students",
    "travis": "npm run lint && npm run build:areas && npm run test:students && npm run karma:ci && npm run build:ci",
    "watch": "npm run watch:lint & npm run watch:test & wait"
  },
  "dependencies": {
    "appdirs": "1.0.0",
    "babel-runtime": "5.8.29",
    "bluebird": "3.0.5",
    "classnames": "2.2.0",
    "combinations-generator": "1.0.1",
    "debug": "2.2.0",
    "fuzzysearch": "1.0.3",
    "humanize-plus": "1.5.0",
    "idb-range": "2.4.0",
    "immutable": "3.7.5",
    "is-safari": "1.0.0",
    "isomorphic-fetch": "2.2.0",
    "js-yaml": "3.4.3",
    "json-stable-stringify": "1.0.0",
    "keymage": "1.1.0",
    "lie": "3.0.1",
    "load-pkg": "2.0.1",
    "lodash": "3.10.1",
    "marked": "0.3.5",
    "node-uuid": "1.4.3",
    "ord": "0.1.1",
    "plur": "2.0.0",
    "present": "0.0.6",
    "react": "0.13.3",
    "react-codemirror": "0.1.7",
    "react-dnd": "1.1.8",
    "react-document-title": "2.0.1",
    "react-dropzone": "2.2.4",
    "react-redux": "3.1.0",
    "react-router": "0.13.4",
    "redux": "3.0.4",
    "reflux": "0.3.0",
    "sha1": "1.1.1",
    "stickyfill": "1.1.1",
    "sto-course-related-data": "4.0.2",
    "sto-helpers": "4.2.12",
    "sto-sis-time-parser": "2.1.0",
    "text-table": "0.2.0",
    "treo": "0.5.0"
  },
  "devDependencies": {
    "autoprefixer": "6.0.3",
    "babel": "5.8.29",
    "babel-core": "6.0.15",
    "babel-eslint": "4.1.3",
    "babel-loader": "5.3.2",
    "babel-plugin-inline-package-json": "1.0.1",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "1.1.1",
    "chai": "3.4.0",
    "coveralls": "2.11.4",
    "css-loader": "0.21.0",
    "dom-storage": "2.0.1",
    "eslint": "1.8.0",
    "eslint-plugin-react": "3.6.3",
    "extract-text-webpack-plugin": "0.9.0",
    "file-loader": "0.8.4",
    "glob": "5.0.15",
    "graceful-fs": "4.1.2",
    "isparta": "3.5.0",
    "js-beautify": "1.5.10",
    "json-loader": "0.5.3",
    "junk": "1.0.2",
    "karma": "0.13.14",
    "karma-chrome-launcher": "0.2.1",
    "karma-firefox-launcher": "0.1.6",
    "karma-mocha": "0.2.0",
    "karma-webpack": "1.7.0",
    "latest-release": "1.0.2",
    "matcha": "0.6.0",
    "mkdirp": "0.5.1",
    "mocha": "2.3.3",
    "node-libs-browser": "0.5.3",
    "node-sass": "3.4.1",
    "nomnom": "1.8.1",
    "pegjs": "0.9.0",
    "postcss": "5.0.10",
    "postcss-loader": "0.7.0",
    "pretty-hrtime": "1.0.1",
    "pretty-ms": "2.1.0",
    "react-transform-catch-errors": "1.0.0",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.1.1",
    "redux-devtools": "2.1.5",
    "rimraf": "2.4.3",
    "sass-loader": "3.1.1",
    "sparkly": "2.0.1",
    "style-loader": "0.13.0",
    "url-loader": "0.5.6",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.12.1"
  }
}
