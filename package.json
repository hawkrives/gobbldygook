    "react-input-autosize": "1.1.0",
    "react-modal": "1.7.1",
    "react-radio-group": "3.0.1",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "react-select": "1.0.0-rc.3",
    "redux": "3.6.0",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.2.0",
    "redux-undo": "0.6.1",
    "serialize-error": "2.1.0",
