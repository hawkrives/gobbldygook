    "react-input-autosize": "1.1.0",
    "react-modal": "1.4.0",
    "react-radio-group": "3.0.1",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-select": "1.0.0-beta13",
    "redux": "3.6.0",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.1.0",
    "redux-undo": "0.6.1",
    "serialize-error": "2.0.0",
    "sparkly": "3.1.2",
    "stabilize": "1.0.0",
