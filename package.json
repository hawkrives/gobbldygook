{
  "name": "gobbldygook",
  "version": "3.0.0-beta3",
  "description": "A course scheduling app for St. Olaf College",
  "main": "src/index.js",
  "private": true,
  "repository": "hawkrives/gobbldygook",
  "author": "Hawken MacKay Rives",
  "license": "MIT",
  "COURSE_INFO": "../sto-course-data/build/info.json",
  "AREA_INFO": "./build/areas/info.json",
  "config": {
    "beautify": "-f - --end-with-newline --indent-with-tabs",
    "mocha": "test/areas test/models test/helpers test/ducks"
  },
  "scripts": {
    "benchmark": "babel-node -- ./benchmarks/students-benchmark.js",
    "build": "env NODE_ENV=production webpack --config .webpackrc --bail --production --progress",
    "build:dev": "env NODE_ENV=dev webpack --config .webpackrc --bail --progress",
    "build:areas": "./bin/compile-areas area-data/ --out-dir build/areas/",
    "build:ci": "npm run build -- --no-progress",
    "build:peg": "pegjs < src/lib/parse-hanson-string.pegjs | babel --compact 'true' | js-beautify $npm_package_config_beautify > src/lib/parse-hanson-string.js",
    "clean": "rimraf build/",
    "copy": "mkdirp build/ && cp -rf src/static/* build/ && npm run copy:areas && npm run copy:icons",
    "copy:icons": "svg-sprite-generate -d src/icons/ionicons/ -o build/ionicons.svg",
    "copy:areas": "cp -rf ./area-data/ build/areas/ && ./bin/area-package-maker build/areas/ --save",
    "cover": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- $npm_package_config_mocha",
    "cover:view": "npm run cover && open coverage/lcov-report/index.html",
    "coveralls": "npm run cover && coveralls < ./coverage/lcov.info",
    "eslint": "npm run lint",
    "lint": "eslint bin/ src/ test/",
    "mocha": "env NODE_ENV=test mocha $npm_package_config_mocha",
    "prebuild": "npm run copy",
    "precopy": "npm run clean",
    "prestart": "npm run copy",
    "profile": "cat profile/README.md",
    "start": "env NODE_ENV=dev webpack-dev-server --config .webpackrc --progress",
    "stats": "env NODE_ENV=production npm run stats:base",
    "stats:base": "webpack --config .webpackrc --bail --profile --json > stats.json && open http://webpack.github.io/analyse/",
    "stats:dev": "env NODE_ENV=dev npm run stats:base",
    "test": "npm run mocha",
    "test:students": "./bin/test-students"
  },
  "dependencies": {
    "appdirs": "1.0.0",
    "babel-runtime": "5.8.29",
    "bluebird": "3.0.6",
    "classnames": "2.2.1",
    "codemirror": "5.9.0",
    "combinations-generator": "1.0.1",
    "debug": "2.2.0",
    "fuzzysearch": "1.0.3",
    "history": "1.13.1",
    "humanize-plus": "1.5.0",
    "idb-range": "3.1.2",
    "immutable": "3.7.5",
    "is-safari": "1.0.0",
    "isomorphic-fetch": "2.2.0",
    "js-yaml": "3.4.6",
    "json-stable-stringify": "1.0.0",
    "keymage": "1.1.2",
    "load-pkg": "3.0.0",
    "lodash": "3.10.1",
    "marked": "0.3.5",
    "ord": "0.1.1",
    "plur": "2.1.1",
    "present": "0.0.6",
    "react": "0.14.3",
    "react-codemirror": "0.2.2",
    "react-dnd": "2.0.2",
    "react-dnd-html5-backend": "2.0.0",
    "react-document-title": "2.0.1",
    "react-dom": "0.14.3",
    "react-dropzone": "3.2.1",
    "react-gateway": "1.1.0",
    "react-modal2": "2.0.1",
    "react-redux": "4.0.0",
    "react-router": "1.0.1",
    "redux": "3.0.4",
    "reflux": "0.3.0",
    "sha1": "1.1.1",
    "sto-course-related-data": "4.0.2",
    "sto-sis-time-parser": "2.2.2",
    "text-table": "0.2.0",
    "treo": "0.5.1",
    "uuid": "2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "6.1.2",
    "babel": "5.8.34",
    "babel-core": "6.3.15",
    "babel-eslint": "4.1.6",
    "babel-loader": "5.3.3",
    "babel-plugin-inline-package-json": "1.0.1",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "1.1.1",
    "chai": "3.4.1",
    "coveralls": "2.11.4",
    "css-loader": "0.23.0",
    "dom-storage": "2.0.1",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.11.3",
    "extract-text-webpack-plugin": "0.9.1",
    "file-loader": "0.8.5",
    "glob": "6.0.1",
    "graceful-fs": "4.1.2",
    "isparta": "3.5.3",
    "js-beautify": "1.5.10",
    "json-loader": "0.5.4",
    "junk": "1.0.2",
    "latest-release": "1.0.2",
    "mkdirp": "0.5.1",
    "mocha": "2.3.4",
    "node-libs-browser": "0.5.3",
    "node-sass": "3.4.2",
    "nomnom": "1.8.1",
    "pegjs": "0.9.0",
    "postcss": "5.0.12",
    "postcss-loader": "0.8.0",
    "pretty-hrtime": "1.0.1",
    "pretty-ms": "2.1.0",
    "react-transform-catch-errors": "1.0.0",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.2.0",
    "redux-devtools": "2.1.5",
    "rimraf": "2.4.4",
    "sass-loader": "3.1.2",
    "sparkly": "2.0.1",
    "style-loader": "0.13.0",
    "svg-sprite-generator": "0.0.7",
    "url-loader": "0.5.7",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0",
    "worker-loader": "0.7.0"
  }
}
